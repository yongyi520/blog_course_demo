<h1 id="page_title">Stuff I've Written</h1>


<div id="post_wrapper" class="skinny_wrapper">
  <%= @post.each do |post| %>
      <div class="post">
        <p class="date"><%= post.created_at.strftime('%A, %B %d') %></p>
        <h2><a href="#"><%= post.title %></a></h2>
        <hr>
      </div>
  <% end %>

</div>
